## ./aws/config

[default]
region = eu-central-1
aws_access_key_id=...
aws_secret_access_key=...
mfa_serial=arn:aws:iam::12345:mfa/mynamehere

[profile staging]
role_arn = arn:aws:iam::12345:role/RoleHere
source_profile = myprofile
region = eu-central-1
mfa_serial=arn:aws:iam::12345:mfa/MfaNameHere


